{
    module inner = {
        pi = 3.14,
    },

    type Person = forall a . { name : Str | a },

    person_a = { name = "Alice" },

    person_b = { person_a | name = "Bob" },

    #eine_liste : List Str = ["eins", "zwei", "drei"],

    #main = [1, 2, 3],

    nested_a = { id = 1, info = { person = { age = 30 } } },

    nested_b = { nested_a.info | person.age += 1 },

    #main = nested_b,

    #main = person_b.name,

    main = inner::pi,
}
